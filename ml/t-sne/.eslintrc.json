{
    "extends": ["eslint:recommended"],
    "plugins": [],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2020
    },
    "env": {
        "browser": false,
        "node": true,
        "es6": true
    },
    "globals": {},
    "rules": {
        "semi": ["error", "always"],
        "linebreak-style": ["error", "unix"],
        "no-unused-vars": "warn"
    }
}